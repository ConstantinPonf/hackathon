  "source": "pragma solidity 0.5;\ncontract CoffeeExchange {\n    uint256 public counter;\n    \n    constructor() public {\n        counter = 1;\n    }\n    \n    function getCounter() public view returns (uint256) {\n        return counter;\n    }\n    \n    function increment() public {\n        counter++;\n    }\n}",
